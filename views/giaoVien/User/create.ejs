<body ng-app="user" ng-controller="userController" style="background-image: url(../images/hero-bg.jpg);padding: 20px">
    <% include ../../header.ejs %>
<br />
<div class="container" style="margin-top:5%" >
    <div class="col-md-3"></div>
    <div class="col-md-6" style="width:100%">
         <div class="row myborder" style="background: rgba(255,255,255,.7)">
             <h1 style="color: #19c880;font-size: 55px;text-align: center; margin: initial; margin-bottom: 10px;">Thêm tài khoản</h1><hr>
            <div class="input-group margin-bottom-20">
                <span class="input-group-addon"><i class="glyphicon glyphicon-user mycolor"></i></span>
                <input ng-model="tenNguoiDung"  size="60" maxlength="255" class="form-control" placeholder="Nhập tên"  type="text">                                                        </div>
            <div class="input-group margin-bottom-20">
                <span class="input-group-addon"><i class="glyphicon glyphicon-lock mycolor"></i></span>
                <input ng-model="password"  size="60" maxlength="255" class="form-control" placeholder="Password"type="password">                                    </div>
            <div class="input-group margin-bottom-20">
                <span class="input-group-addon"><i class="glyphicon glyphicon-envelope mycolor"></i></span>
                <input ng-model="MSHS"  size="60" maxlength="255" class="form-control" placeholder="mã số học sinh" type="text">                                    </div>
            <div class="input-group margin-bottom-20">
                <span class="input-group-addon"><i class="glyphicon glyphicon-phone mycolor"></i></span>
                <input ng-model="SDT" size="60" maxlength="255" class="form-control" placeholder="Số điện thoại"type="text">                                    </div>
            <div class="row">
                <div class="col-md-12">
                    <button style="margin-left: 42%;font-size: 25px;border-radius: 10px ; background-color:#19c880"class="btn-u pull-left" type="submit" ng-click="tao()">Thêm tài khoản</button>
                </div>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>
      </div>
    </body>
